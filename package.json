    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.1",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths": "^3.14.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
